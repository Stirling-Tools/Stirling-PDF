{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "paths": {
      "@app/*": [
        "src/proprietary/*",
        "src/core/*"
      ],
      "@core/*": ["src/core/*"]
    }
  },
  "exclude": [
    "src/core/**/*.test.ts*",
    "src/core/**/*.spec.ts*",
    "src/desktop",
    "node_modules"
  ]
}
